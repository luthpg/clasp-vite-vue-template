{
  "name": "clasp-vite-vue-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tsc": "vue-tsc",
    "build": "vite build",
    "preview": "vite preview",
    "push": "clasp push",
    "push:watch": "clasp push -w",
    "deploy": "dotenv -- bash -c 'clasp deploy -i $GAS_DEPLOYMENT_ID'",
    "deploy:first": "clasp deploy",
    "open:script": "clasp open",
    "open:web": "clasp open --webapp",
    "test": "jest --silent=true --coverage",
    "test:console": "jest",
    "lint": "eslint --ext .ts,.vue src/ && eslint --ext .ts,.vue server/",
    "format": "prettier --write \"**/*.+(js|json|ts|vue)\""
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.59",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.10.0",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-singlefile": "^0.13.5",
    "vue-tsc": "^1.2.0"
  }
}
